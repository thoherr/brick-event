<h1>Listing attendee_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @attendee_types.each do |attendee_type| %>
  <tr>
    <td><%= attendee_type.name %></td>
    <td><%= attendee_type.description %></td>
    <td><%= link_to 'Show', attendee_type %></td>
    <td><%= link_to 'Edit', edit_attendee_type_path(attendee_type) %></td>
    <td><%= link_to 'Destroy', attendee_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

